{
  "_version": "1.42.0",
  "sap.app": {
    "id": "project3",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.1",
      "toolsId": "d1a40033-ed7a-4cb8-b161-66f0a52632fc"
    },
    "dataSources": {
      "requestService": {
        "uri": "/request/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "companyService": {
        "uri": "/company/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
      
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project3.i18n.i18n"
        }
      },
      "Request" : {
        "dataSource": "requestService",
        "preload": true,
        "settings": {
          "operationMode" : "Server",
          "earlyRequests" : true,
          "synchronizationMode" : "None",
          "autoExpandSelect" : true
        }
      },
      "Vendor" : {
        "dataSource": "companyService",
        "preload": true,
        "settings": {
          "operationMode" : "Server",
          "earlyRequests" : true,
          "synchronizationMode" : "None",
          "autoExpandSelect" : true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "project3.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "GridCompany",
          "pattern": "",
          "target": [
            "GridCompany"
          ]
        },
        {
          "name": "ResponsiveCompany",
          "pattern": "ResponsiveCompany",
          "target": [
            "ResponsiveCompany"
          ]
        },
        {
          "name": "Routeapp",
          "pattern": "Routeapp",
          "target": [
            "Targetapp"
          ]
        },
        {
          "name": "AddCompany",
          "pattern": "AddCompany/{num}",
          "target": [
            "AddCompany"
          ]
        },
        {
          "name": "CompanyDetail",
          "pattern": "CompanyDetail/{num}/{table}",
          "target": [
            "CompanyDetail"
          ]
        }
        
        
      ],
      "targets": {
        "GridCompany": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "GridCompany",
          "viewName": "GridCompany"
        }, 
        "ResponsiveCompany": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "ResponsiveCompany",
          "viewName": "ResponsiveCompany"
        }, 
        "Targetapp": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "app",
          "viewName": "app"
        },
        "AddCompany": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "AddCompany",
          "viewName": "AddCompany"
        },
        "CompanyDetail" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "CompanyDetail",
          "viewName": "CompanyDetail"
        }
      }
    },
    "rootView": {
      "viewName": "project3.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
